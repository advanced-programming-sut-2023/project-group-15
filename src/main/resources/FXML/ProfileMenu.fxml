<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="349.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.userView.ProfileMenu">
   <children>
      <StackPane prefHeight="349.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="318.0" prefWidth="149.0" spacing="20.0" StackPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onMouseClicked="#profileDisplay" prefHeight="30.0" prefWidth="150.0" text="profile display" />
                  <Button mnemonicParsing="false" onMouseClicked="#changeUsername" prefHeight="30.0" prefWidth="150.0" text="change username" />
                  <Button mnemonicParsing="false" onMouseClicked="#changePassword" prefHeight="30.0" prefWidth="150.0" text="change password" />
                  <Button contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#changeSlogan" prefHeight="30.0" prefWidth="150.0" text="change slogan" />
                  <Button mnemonicParsing="false" onMouseClicked="#chooseAvatar" prefHeight="30.0" prefWidth="150.0" text="choose new avatar" />
                  <Button mnemonicParsing="false" onMouseClicked="#changeEmail" prefHeight="30.0" prefWidth="150.0" text="change email" />
                  <Button mnemonicParsing="false" onMouseClicked="#changeNickname" prefHeight="30.0" prefWidth="150.0" text="change nickname" />
                  <Button alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#removeSlogan" prefHeight="30.0" prefWidth="150.0" text="remove slogan" />
                  <Button mnemonicParsing="false" onMouseClicked="#showScoreboard" prefHeight="30.0" prefWidth="150.0" text="show scoreboard" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
